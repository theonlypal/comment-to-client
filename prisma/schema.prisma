generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Lead {
  id             String   @id @default(cuid())
  createdAt      DateTime @default(now())
  updatedAt      DateTime @updatedAt

  fullName       String
  email          String
  phone          String?

  igUserId       String?
  igUsername     String?
  igCommentId    String?
  igMediaId      String?
  campaign       String?

  notes          String?
  source         String?

  @@index([email])
  @@index([createdAt])
}
